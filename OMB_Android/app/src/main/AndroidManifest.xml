        </activity> <!-- Actividad Main -->
        <activity
                android:name=".Login_Activity"
                android:theme="@style/No_barra_titulo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>